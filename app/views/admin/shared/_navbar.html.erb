      <% end %>
    </div>

    <nav class="db dtc-ns v-mid w-100 tl tr-ns mt2 mt0-ns">
      <%= link_to t(".incoming_packages"), admin_incoming_packages_path, class: "f6 fw6 link black-70 dim mr2 mr3-ns dib v-mid" %>
      <%= link_to t(".packages"), admin_packages_path, class: "f6 fw6 link black-70 dim mr2 mr3-ns dib v-mid" %>
      <%= link_to t(".shipments"), admin_shipments_path, class: "f6 fw6 link black-70 dim mr2 mr3-ns dib v-mid" %>
      <%= link_to t(".users"), admin_users_path, class: "f6 fw6 link black-70 dim mr2 mr3-ns dib v-mid" %>

      <span id="tippy-navbar-admin" class="dib v-mid pointer mr2 mr3-m mr4-l pt1">
        <svg width="20" height="20" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"> <rect x="2" y="4" width="16" height="1"></rect> <rect x="2" y="9" width="16" height="1"></rect> <rect x="2" y="14" width="16" height="1"></rect></svg>
      </span>
      <div id="tippy-navbar-admin-template" style="display: none;">
        <ul class="list pl0 ml0">
          <li>
            <%= link_to t(".news"), admin_news_index_path, class: "db tl f6 fw6 link black-70 w-100 pa3 bb br2 br--top b--black-10 hover-bg-near-white" %>
          </li>
          <li>
            <%= link_to t(".pages"), admin_pages_path, class: "db tl f6 fw6 link black-70 w-100 pa3 bb b--black-10 hover-bg-near-white" %>
          </li>
          <li>
            <%= link_to t(".site_pricing"), edit_admin_site_pricing_path, class: "db tl f6 fw6 link black-70 w-100 pa3 bb b--black-10 hover-bg-near-white" %>
          <li>
            <%= link_to t(".site_setting"), edit_admin_site_setting_path, class: "db tl f6 fw6 link black-70 w-100 pa3 bb b--black-10 hover-bg-near-white" %>
          </li>
          <li>
            <%= link_to t(".homepage"), root_path, 'data-turbolinks': "false", class: "db tl f6 fw6 link black-70 w-100 pa3 bb br2 br--bottom b--black-10 hover-bg-near-white" %>
          </li>
        </ul>
      </div>

      <span id="tippy-navbar-user" class="dib v-mid pointer">
        <svg width="20" height="20" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"> <circle fill="none" stroke="#000" stroke-width="1.1" cx="9.9" cy="6.4" r="4.4"></circle> <path fill="none" stroke="#000" stroke-width="1.1" d="M1.5,19 C2.3,14.5 5.8,11.2 10,11.2 C14.2,11.2 17.7,14.6 18.5,19.2"></path></svg>
      </span>
      <div id="tippy-navbar-user-template" style="display: none;">
        <ul class="list pl0 ml0">
          <li>
            <%= link_to t("navbar.edit_account"), edit_user_registration_path, class:"db tl f6 fw6 link black-70 w-100 pa3 bb br2 br--top b--black-10 hover-bg-near-white" %>
          </li>
          <li>
            <%= link_to t("navbar.logout"), destroy_user_session_path, method: :delete, class: "db tl f6 fw6 link black-70 w-100 pa3 br2 br--bottom hover-bg-near-white" %>
          </li>
        </ul>
      </div>
